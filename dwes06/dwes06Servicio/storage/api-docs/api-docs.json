                                                    },
                                                    "nombre": {
                                                        "type": "string",
                                                        "example": "Taller de lectura"
                                                    },
                                                    "descripcion": {
                                                        "type": "string",
                                                        "example": "Taller de lectura en la biblioteca"
                                                    },
                                                    "dia_semana": {
                                                        "type": "string",
                                                        "example": "L"
                                                    },
                                                    "hora_inicio": {
                                                        "type": "string",
                                                        "example": "10:00:00"
                                                    },
                                                    "hora_fin": {
                                                        "type": "string",
                                                        "example": "11:00:00"
                                                    },
                                                    "created_at": {
                                                        "type": "string",
                                                        "example": "2023-02-23T00:09:16.000000Z"
                                                    },
                                                    "updated_at": {
                                                        "type": "string",
                                                        "example": "2023-02-23T12:33:45.000000Z"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "NOT FOUND",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "La ubicación no existe"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
